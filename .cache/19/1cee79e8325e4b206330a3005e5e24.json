top: 30%;\n        }\n        @media (min-width: 1200px){\n            width: 45vw;\n            top: 10%;\n            right: -5%;\n        }\n    }\n}\nmain{\n    max-width: 1400px;\n    margin: 5rem auto 0;\n    padding: 4rem 0;\n    .gallery{\n        position: relative;\n        font-family: Impact;\n        color: #f1f1f1;\n        opacity: 0.8;\n        &__title{\n            text-transform: uppercase;\n            font-size: 5rem;\n            position: absolute;\n            z-index: 2;\n            top: -10%;\n            left: 50%;\n            transform: translate(-50%, 0);\n           \n        }\n        article{\n            position: relative;\n          \n            .article__title{\n                position: absolute;\n                font-size: 3rem;\n                z-index: 2;\n                top:50%;\n                left: 50%;\n                transform: translate(-50%, -50%);\n                @media (min-width: 768px){\n                    font-size: 4rem;\n                }\n            }\n         \n        }\n        article:not(:first-of-type){\n            margin: 5em 0;\n        }\n    }\n    .aboutme{\n        display: flex;\n        flex-direction: column;\n        align-items: center;\n        color: #f1f1f1;\n    &__title{\n        font-size: 2.5rem;\n        text-transform: uppercase;\n        text-align: center;\n        font-family: Impact;\n        @media (min-width: 768px){\n            font-size: 3rem;\n        }\n    }\n    &__article{\n        display: flex;\n        flex-direction: column;\n        margin: 2em 0;\n        place-items: center;\n    }\n    &__article:nth-child(even){\n        flex-direction: column-reverse;\n    }\n    &__description{\n        margin: 1em 1em;\n        font-family: Geneva;\n        max-width: 400px;\n    }\n    &__img{\n        width: clamp(250px, 50vw, 500px);\n    }\n    }\n    .contact{\n        margin: 4em 0;\n        display: flex;\n        flex-direction: column;\n        align-items: center;\n        color:#f1f1f1;\n        &__title{\n            text-transform: uppercase;\n            font-family: Impact;\n            font-size: 2.5rem;\n            @media (min-width: 768px){\n                font-size: 3rem;\n            }\n        }\n        &__medias{\n            display: flex;\n            flex-direction: row;\n            align-items: center;\n            margin:1em;\n        }\n        p{\n            font-family: Geneva;\n            margin: 1em;\n            max-width: 500px;\n            text-align: center;\n        }\n    }\n    \n}\n.footer{\n    min-height: 5vh;\n    padding: 0.5rem;\n    background-color: #f1f1f1;\n    p {\n        margin: 0 auto;\n        text-align: center;\n        max-width: 800px;\n        color: #999999;\n    }\n}"},"lineCount":null}},"error":null,"hash":"e4aaaddb106ce3f08da4a8dddeef88bc","cacheData":{"env":{}}}